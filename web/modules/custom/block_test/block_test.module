<?php

/**
 * @file
 * Set redirection to custom welcome page.
 */

use Drupal\Core\Form\FormStateInterface;
use Drupal\Core\Url;

/**
 * Implements hook_form_FORM_ID_alter().
 */
function block_test_form_user_login_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  // Alter login form and add own custom submit handler.
  $form['#submit'][] = 'user_login_form_submit_callback';
}

/**
 * Custom submit handler for login form.
 */
function user_login_form_submit_callback($form, FormStateInterface $form_state) {
  $url = Url::fromUri('http://www.ft2023-356.com/custom-welcome-page');
  $form_state->setRedirectUrl($url);
}
